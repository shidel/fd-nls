INVALID=%r "Ungültiger Befehl: `" %s "'"
BAD_INT="Ungültiger numerischer Wert."
BAD_VAL="Wert außerhalb des Bereiches."

DRIVE=Laufwerk %_ %c: (ID 0x %b ) %_
DRIVE_TYPE=%_ (ID 0x %b )
DRIVE_TYPE_00=Unbekannter Laufwerkstyp
DRIVE_TYPE_01='5,25"'-360-KB-Laufwerk
DRIVE_TYPE_02='5,25"'-1,2-MB-Laufwerk
DRIVE_TYPE_03='3,5"'-720-KB-Laufwerk
DRIVE_TYPE_04='3,5"'-1,44-MB-Laufwerk
DRIVE_TYPE_05=Sonstiger Laufwerkstyp

DRIVE_SPEC=%i %_ Byte, %_ %I %_ Sektoren, %_ %I %_ Spuren, %_ %I %_ Seite(n)

PASS_LIMIT=Bis zu %_ %i %_ weitere Durchläufe zum Lesen der Diskette erlaubt
BUFFERS=%i %_ Byte Diskettenpuffer, %_ %i %_ Byte Spurergebnispuffer

PASS=Diskettenlesedurchlauf # %i. %_ %i %_ Sektor(en) verbleibend.

READ_FAST=LESE SPUR: %_ %i, KOPF: %_ %i
READ_SLOW=LESE SPUR: %_ %i, KOPF: %_ %i, SEKTOR: %_ %i

FAILED=Abbilderstellung fehlgeschlagen.
ABORTED=Abbilderstellung abgebrochen.
COMPLETED=Abbilderstellung abgeschlossen.

; Mit einer kleinen Änderung sind diese BIOS- und DOS-Fehlermeldungstexte
; eine abgespeckte Version von denen, die online von einem großartigen DOS
; und ASM verfügbar sind
; Ressource unter http://stanislavs.org/helppc/idx_interrupt.html

BErr=%r "BIOS-Fehlercode: 0x" %b %_ --> %_
BErr_01=Fehlerhafter Befehl an Treiber übergeben
BErr_02=Adressmarkierung nicht gefunden oder fehlerhafter Sektor
BErr_03=Schreibschutzfehler
BErr_04=Sektor nicht gefunden
BErr_05=Zurücksetzen der Festplatte fehlgeschlagen
BErr_06=Diskette gewechselt oder entfernt
BErr_07=Fehlerhafte Festplattenparametertabelle
BErr_08=DMA-Überlauf
BErr_09=DMA-Zugriff über 64 KiB-Grenze
BErr_0a=Fehlerhaftes Festplattensektorkennzeichen
BErr_0b=Fehlerhafter Festplattenzylinder
BErr_0c=Nicht unterstützte Spur
BErr_0d=Ungültige Anzahl von Sektoren bei Festplattenformatierung
BErr_0e=Festplattensteuerungsdatenadressmarkierung entdeckt
BErr_0f=Festplatten-DMA-Schlichtungsebene außerhalb des Bereiches
BErr_10=ECC/CRC-Fehler beim Lesen
BErr_11=Wiederherstellbarer Festplattendatenfehler, Daten korrigiert durch ECC
BErr_20=Controllerfehler
BErr_40=Positionierungsfehler
BErr_80=Laufwerk nicht bereit
BErr_aa=Festplatte nicht bereit
BErr_bb=Undefinierter Festplattenfehler
BErr_cc=Festplattenschreibfehler auf ausgewähltem Laufwerk
BErr_e0=Festplattenstatusfehler
BErr_ff=Abtastungsvorgang fehlgeschlagen

DErr=%r "DOS-Fehlercode: 0x" %b %_ --> %_
DErr_01=Ungültige Funktion
DErr_02=Datei nicht gefunden
DErr_03=Pfad nicht gefunden
DErr_04=Zu viele geöffnete Dateien
DErr_05=Zugriff verweigert
DErr_06=Ungültiges Handle
DErr_08=Unzureichender Speicher
DErr_0f=Ungültiges Laufwerk angegeben
DErr_15=Laufwerk nicht bereit
